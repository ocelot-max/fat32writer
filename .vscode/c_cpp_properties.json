                "${workspaceFolder}/**",
                "${workspaceFolder}/src/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "E:/MinGW64/bin/gcc.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
